##Determine whether the triangle is Equilateral or Isosceles or Scalene


#include <iostream>
using namespace std;

int main() {
	int x,y,z;
	cin>>x>>y>>z;
	if(x == y and y== z){
		cout<<"Equilateral";
	}
	else if(x==y or x==z or y==z){
		cout<<"Isosceles Triangle";
	}
	else{
		cout<<"Scalene Triangle";
	}
	return 0;
}
